<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <changeSet id="001" author="romario">
        <createTable tableName="cars">
            <column name="car_id" type="UUID">
                <constraints primaryKey="true" primaryKeyName="car_primary_key"/>
            </column>
            <column name="license_plate" type="VARCHAR(255)"/>
            <column name="enrollment_date" type="TIMESTAMP WITHOUT TIME ZONE"/>
            <column name="brand" type="VARCHAR(255)"/>
            <column name="type" type="VARCHAR(255)"/>
            <column name="color" type="VARCHAR(255)"/>
            <column name="origin_country" type="VARCHAR(255)"/>
            <column name="motor" type="VARCHAR(255)"/>
            <column name="chassis" type="VARCHAR(255)"/>
        </createTable>
        <createTable tableName="days">
            <column name="day_id" type="UUID">
                <constraints primaryKey="true" primaryKeyName="day_primary_key"/>
            </column>
            <column name="day" type="VARCHAR(255)"></column>
        </createTable>
        <createTable tableName="day_numbers">
            <column name="day_number_id" type="UUID">
                <constraints primaryKey="true" primaryKeyName="day_number_primary_key"/>
            </column>
            <column name="number" type="INTEGER"></column>
        </createTable>
    </changeSet>
